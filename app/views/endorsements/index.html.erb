	<h1><%= @content.title %></h1>
	
	<p class="lead"><%= @content.content %></p>
	
<% unless @orgs.blank? %>
<h2>Organizations</h2> <% end %>
	<% @orgs.each do |e| %>
	<p><%= e.organization %>  <% if current_user %> <span class="label label-warning"> <%= e.ranking %></span>   <span class="edit alert edit-endorsement" >
	   <%= link_to ' edit', edit_endorsement_path(e), class: "admin-edit fa fa-edit" %> |  <%= link_to ' delete', endorsement_path(e), class: "admin-edit fa fa-trash", :method => :delete, data: { confirm: ('messages.confirm_delete'), ok: ('buttons.ok'), cancel: ('buttons.cancel')} %></p>
</span>
<% end %></p>
<% end %>



<% unless @electeds.blank? %>	
<h2>Elected Officials</h2>   
<% @electeds.each do |e| %>
  
 <p><strong><%= e.firstname %> <%= e.lastname %></strong>  <%= e.title %> <% if current_user %> 
	<span class="label label-warning"> <%= e.ranking %></span>
	  <span class="edit alert edit-endorsement" >
		   <%= link_to ' edit', edit_endorsement_path(e), class: "admin-edit fa fa-edit" %> |  <%= link_to ' delete', endorsement_path(e), class: "admin-edit fa fa-trash", :method => :delete, data: { confirm: ('messages.confirm_delete'), ok: ('buttons.ok'), cancel: ('buttons.cancel')} %><% end %> </p>
  </span>
<% end %>
<% end %>		 


<% unless @individuals.blank? %>
<h2>Community Leaders</h2> 
<% @individuals.each do |e| %>

<p><strong><%= e.firstname %> <%= e.lastname %></strong>   <%= e.title %>  <% if current_user %>   <span class="edit alert edit-endorsement" >
<%= link_to ' edit', edit_endorsement_path(e), class: "admin-edit fa fa-edit" %> |  <%= link_to ' delete', endorsement_path(e), class: "admin-edit fa fa-trash", :method => :delete, data: { confirm: ('messages.confirm_delete'), ok: ('buttons.ok'), cancel: ('buttons.cancel')} %><% end %> </p>
</span></p></span>
<% end %>	
<% end %>	

		
	
		
	
		
		<p>	<a href="#" class="crunchify-top">↑Back to top</a>
			
			
			
			
<% content_for :rh do %>

		<% if current_user %><div class="edit alert" >
		 <i> <%= link_to ' Add an endorsement  ', new_endorsement_path, class: "admin-edit fa fa-user-plus", dismissible: true %> </i>
 
		 <button class="close" data-dismiss="alert" type="button">
		     <span aria-hidden="true">×</span><span class="sr-only">Close</span>
		   </button>
   
		 </div>
		 
<% end %>
<% end %>
		